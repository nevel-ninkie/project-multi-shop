{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },

  "plugins": ["@vue"],
  "rules": {
    "no-unused-vars": "warn"
  }
}
